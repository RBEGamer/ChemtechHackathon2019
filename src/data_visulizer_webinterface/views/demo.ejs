<script>


function create_chart(array){
	var color = Chart.helpers.color;
		var barChartData = {
			labels: ['T-9', 'T-8', 'T-7', 'T-6', 'T-5', 'T-4', 'T-3', 'T-1', 'T-0'],
			datasets: [{
				label: 'Err 1',
				borderWidth: 1,
				data: array
			}]
    };
    var ctx = document.getElementById('canvas_err').getContext('2d');
			window.myBar = new Chart(ctx, {
				type: 'bar',
				data: barChartData,
				options: {
					responsive: true,
					legend: {
						position: 'top',
					},
					title: {
						display: false,
						text: ''
					}
				}
			});


}
	

function create_img(place){
  var nfc = 0;

if(place == "left"){
  $('#placeimg').attr('src', '/img/left.png');
nfc = 1;
}else if(place == "normal"){
 $('#placeimg').attr('src', '/img/normal.png');
}else if(place == "right"){
   $('#placeimg').attr('src', '/img/right.png');
nfc = 1;
}else if(place== "shaft"){
   $('#placeimg').attr('src', '/img/shaft.png');
nfc = 1;
}else{
 $('#placeimg').attr('src', '/img/normal.png');
}

 $("#notify").html(" <span class='notification'>"+nfc+"</span>");
    $("#mc_badge").html(nfc);


}




